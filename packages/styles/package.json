{
  "name": "@mono/styles",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./scss": {
      "default": "./src/base-styles.scss"
    },
    "./scss/*": {
      "default": "./src/*"
    },
    "./dist/scss": {
      "default": "./dist/base-styles.scss"
    },
    "./dist/scss/*": {
      "default": "./dist/*"
    },
    "./dist/*": {
      "default": "./dist/*"
    }
  },
  "nx": {
    "name": "styles",
    "tags": [
      "type:styles",
      "scope:shared"
    ]
  },
  "dependencies": {
    "sass": "^1.81.0"
  },
  "peerDependencies": {
    "sass": "^1.81.0"
  }
}
