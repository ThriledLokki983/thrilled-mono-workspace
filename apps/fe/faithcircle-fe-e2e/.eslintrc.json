    "!**/*",
    "out-tsc/**/*",
    "test-results/**/*",
    "coverage/**/*",
    "*.d.ts"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
